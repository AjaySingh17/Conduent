pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        echo 'starting Build'
      }
    }
    stage('Deploy') {
      steps {
        echo 'In the deploy stage'
      }
    }
    stage('Test') {
      steps {
        echo 'InAutomated testing phase'
      }
    }
    stage('ProdDeploy') {
      steps {
        echo 'Final Deploy to Prod'
      }
    }
  }
}